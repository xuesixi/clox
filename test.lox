fun vector(x, y) {

    fun getX() {
        return x;
    }

    fun getY() {
        return y;
    }

    fun add(a, b) {
        return vector(x + a, y + b);
    }

    fun dot(v) {
        return x * v("getX")() + y * v("getY")();
    }
    
    fun methods(name) {
        switch (name) {
            case "getX":
                return getX;
            case "getY":
                return getY;
            case "add":
                return add;
            case "dot":
                return dot;
            default:
                return nil;
        }
    }

    return methods;
}

