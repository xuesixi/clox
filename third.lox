
class Animal {
    init(name, age) {
        this.name = name;
        this.age = age;
    }

    run() {
        print f("the animal # is running", this.name);
    }

    new() {
        return Animal(f("son of #", this.name), 1);
    }
}

var dog = Animal("anda", 10);
var cat = dog.new();

var arr = dog, cat;
for (var i = 0; i < arr.length; i += 1) {
    arr[i].run();
}


